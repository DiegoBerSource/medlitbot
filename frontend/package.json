{
  "name": "medlitbot-frontend",
  "version": "1.0.0",
  "description": "Vue 3 Frontend for MedLitBot - Medical Literature AI Classification System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:fast": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "clean": "rm -rf dist node_modules/.vite",
    "dev:host": "vite --host",
    "dev:open": "vite --open",
    "build:analyze": "vite build --mode analyze",
    "full-check": "npm run type-check && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "@vueuse/core": "^10.7.2",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.3.1",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.0.18",
    "tailwindcss": "^3.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "socket.io-client": "^4.7.4",
    "@tanstack/vue-query": "^5.17.19",
    "vue-toastification": "^2.0.0-rc.5",
    "nprogress": "^0.2.0",
    "lodash-es": "^4.17.21",
    "zod": "^3.22.4",
    "@vueuse/nuxt": "^10.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.4",
    "vue-tsc": "^1.8.27",
    "workbox-window": "^7.0.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "pinia",
    "tailwind",
    "medical-ai",
    "django-api",
    "pwa",
    "modern-frontend"
  ],
  "author": "MedLitBot Team",
  "license": "MIT"
}
